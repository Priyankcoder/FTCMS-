{
  "name": "fees_management",
  "version": "1.0.0",
  "description": "Managing student data and fees structure by an institution.",
  "scripts": {
    "babel": "babel --watch --presets es2015 static/home/js static/common/js -d static/dist/js",
    "scss": "cd static && sass --watch common/scss:dist/css home/scss:dist/css",
    "server": "python manage.py runserver 0.0.0.0:8000",
    "start": "concurrently \"npm run scss\" \"npm run babel\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Navaneeth Nagesh",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.0.1",
    "sass": "^1.14.1"
  }
}
